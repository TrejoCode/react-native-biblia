{
	"expo": {
		"name": "La Biblia RV1960",
		"slug": "react-native-biblia",
		"description": "La Biblia Reina Valera 1960 en React Native - Open Source",
		"version": "1.0.0",
		"owner": "Trejocode, tu y todos",
		"githubUrl": "https://github.com/TrejoCode/react-native-biblia.git",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
